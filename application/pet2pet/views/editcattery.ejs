<% include ../views/layout %>
<!-- id, name, address, contact, description, workinghours, image -->
<h1>Edit Cattery Info</h1>
<form name="editcattery" method="POST" action="/editcattery">
    <input type="text" name="id" placeholder="id" value="<%= id%>" hidden="true" />
    <br />
    <input type="text" name="name" placeholder="Name" value="<%= name%>" />
    <br />
    <input type="text" name="address" placeholder="Address" value="<%= address%>" />
    <br />
    <input type="text" name="contact" placeholder="Contact" value="<%= contact%>" />
    <br />
    <input type="text" name="description" placeholder="Description" value="<%= description%>" />
    <br />
    <input type="text" name="workinghours" placeholder="Working hours" value="<%= workinghours%>" />
    <br />
    <input type="text" name="image" placeholder="Image" value="<%= image%>" />
    <br />
    <input type="submit" value="Edit Cattery" class="btn btn-outline-primary btn-sm" />
</form>
